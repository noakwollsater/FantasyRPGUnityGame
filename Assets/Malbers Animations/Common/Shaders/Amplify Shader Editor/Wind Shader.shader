				float4 blendOpSrc80 = temp_output_68_0;
				float4 blendOpDest80 = _WindColor1;
				
				surfaceDescription.Color = float3( 0.5, 0.5, 0.5 );
				surfaceDescription.Emission = ( saturate( 2.0f*blendOpDest80*blendOpSrc80 + blendOpDest80*blendOpDest80*(1.0f - 2.0f*blendOpSrc80) )).rgb;
				surfaceDescription.Alpha = temp_output_68_0.r;
				surfaceDescription.AlphaClipThreshold = _AlphaCutoff;
				surfaceDescription.ShadowTint = float4( 0, 0 ,0 ,1 );
